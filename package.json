    "@commitlint/config-conventional": "^7.3.1",
    "chai": "^4.2.0",
    "eslint": "^5.12.0",
    "eslint-plugin-mocha": "^5.2.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "sinon": "^6.3.5",
